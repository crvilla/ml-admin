{
  "name": "mailo-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "copy-schema": "mkdir -p prisma && cp ../mailo-prisma-schema/prisma/schema.prisma prisma/schema.prisma",
    "generate": "npm run copy-schema && prisma generate --schema=prisma/schema.prisma",
    "migrate": "npm run copy-schema && prisma migrate dev --schema=prisma/schema.prisma"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroui/accordion": "^2.2.20",
    "@heroui/react": "^2.7.11",
    "@heroui/system": "^2.4.19",
    "@heroui/theme": "^2.4.19",
    "@prisma/client": "^6.11.1",
    "bcrypt": "^6.0.0",
    "framer-motion": "^12.23.6",
    "jose": "^6.0.11",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "prisma": "^6.11.1",
    "react": "^19.1.0",
    "react-aria": "^3.41.1",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "sonner": "^2.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "mailo-prisma-schema": "file:../mailo-prisma-schema",
    "tailwindcss": "^4.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
